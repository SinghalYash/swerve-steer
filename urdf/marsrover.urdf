<?xml version="1.0"?>
<robot name="myfirst">
  
  <material name="white">
    <color rgba='1 1 1 0.5'/>
  </material>
  
  <material name='blue'>
    <color rgba='0 1 0 0.5'/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <box size="1.2 0.3 1.2"/>
      </geometry>
      <material name='white'/>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  
  <link name='block_fl'>
    <visual>
        <geometry>
            <box size='0.1 0.1 0.1'/>
        </geometry>
        <material name='blue'/>
    </visual>
  </link>

  <joint name='base_to_flblock' type='continuous'>
    <parent link='base_link'/>
    <child link='block_fl'/>
    <axis xyz='0 0 1'/>
    <origin rpy="0 0 0" xyz='0.535 0.495 -0.2'/>
  </joint>

  <link name="front_left_wheel">
    <visual>
      <geometry>
        <cylinder length="0.13" radius='0.105'/>
      </geometry>
    </visual>
  </link>

  <joint name='blockfl_to_fl' type="continuous">
    <parent link='block_fl'/>
    <child link='front_left_wheel'/>
    <axis xyz='0 0 1'/>
    <origin rpy="1.57075 0 1.57075" xyz='0 0 -0.155'/>
  </joint>

  <link name='block_fr'>
    <visual>
        <geometry>
            <box size='0.1 0.1 0.1'/>
        </geometry>
        <material name='blue'/>
    </visual>
  </link>

  <joint name='base_to_frblock' type='continuous'>
    <parent link='base_link'/>
    <child link='block_fr'/>
    <axis xyz='0 0 1'/>
    <origin rpy="0 0 0" xyz='-0.535 0.495 -0.2'/>
  </joint>
    
  <link name="front_right_wheel">
    <visual>
      <geometry>
        <cylinder length="0.13" radius='0.105'/>
      </geometry>
    </visual>
  </link>

  <joint name='blockfr_to_fr' type="continuous">
    <parent link='block_fr'/>
    <child link='front_right_wheel'/>
    <axis xyz='0 0 1'/>
    <origin rpy="1.57075 0 1.57075" xyz='0 0 -0.155'/>
  </joint>

  <link name='block_bl'>
    <visual>
        <geometry>
            <box size='0.1 0.1 0.1'/>
        </geometry>
        <material name='blue'/>
    </visual>
  </link>

  <joint name='base_to_blblock' type='continuous'>
    <parent link='base_link'/>
    <child link='block_bl'/>
    <axis xyz='0 0 1'/>
    <origin rpy="0 0 0" xyz='0.535 -0.495 -0.2'/>
  </joint>

  <link name="back_left_wheel">
    <visual>
      <geometry>
        <cylinder length="0.13" radius='0.105'/>
      </geometry>
    </visual>
  </link>

  <joint name='blockbl_to_bl' type="continuous">
    <parent link='block_bl'/>
    <child link='back_left_wheel'/>
    <axis xyz='0 0 1'/>
    <origin rpy="1.57075 0 1.57075" xyz='0 0 -0.155'/>
  </joint>

  <link name='block_br'>
    <visual>
        <geometry>
            <box size='0.1 0.1 0.1'/>
        </geometry>
        <material name='blue'/>
    </visual>
  </link>

  <joint name='base_to_brblock' type='continuous'>
    <parent link='base_link'/>
    <child link='block_br'/>
    <axis xyz='0 0 1'/>
    <origin rpy="0 0 0" xyz='-0.535 -0.495 -0.2'/>
  </joint>

  <link name="back_right_wheel">
    <visual>
      <geometry>
        <cylinder length="0.13" radius='0.105'/>
      </geometry>
    </visual>
  </link>

  <joint name='blockbr_to_br' type="continuous">
    <parent link='block_br'/>
    <child link='back_right_wheel'/>
    <axis xyz='0 0 1'/>
    <origin rpy="1.57075 0 1.57075" xyz='0 0 -0.155'/>
  </joint>
</robot>
